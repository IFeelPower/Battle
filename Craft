function infhealth()
    local function healthLoop()
      while infh do
        moveitems:InvokeServer(101, 9, true)
        moveitems:InvokeServer(9, 101, true)
        task.wait()
      end
    end

    if useTaskSpawn then
      task.spawn(healthLoop) -- Runs asynchronously
    else
      healthLoop() -- Runs normally (blocking)
    end
  end

