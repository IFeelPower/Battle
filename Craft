    function cheststealer()
    for i = 36, 62 do
      task.spawn(function()
        game:GetService("ReplicatedStorage").GameRemotes.MoveItem:InvokeServer(i, i - 27, true)
      end)
    end
  end

  function chestdupe(mode)
    if mode == 1 then
      sortitems:InvokeServer(36)
    elseif mode == 2 then
      for i = 36, 62 do
        task.spawn(function()
          sortitems:InvokeServer(i)
        end)
      end
    end
  end

