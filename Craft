        Default = "Select player",
        Options = playerNames,
        Callback = function(PlayerUsername)
lol = PlayerUsername
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace[lol].HumanoidRootPart.CFrame
end})

updatePlayerNames()

