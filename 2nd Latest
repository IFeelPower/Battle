local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Nuke Script - too dangerous",
   LoadingTitle = "Rayfield Interface Suite",
   LoadingSubtitle = "by Sirius",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Big Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Hello"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

local Tab = Window:CreateTab("Nuke", 4483362458) -- Title, Image

local Section = Tab:CreateSection("Cid V2")

local Button = Tab:CreateButton({
 Name = "REAL ATOMIC! - You need to hold V2 All range atomic item",
 Callback = function()
        for i = 1, 10000 do
game:GetService("ReplicatedStorage").CidKageeno.Atomicc:FireServer()
end
   end    
})

local Button = Tab:CreateButton({
 Name = "REAL ATOMIC! with chat",
 Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/IFeelPower/Battle/refs/heads/main/Atomic"))()
   end    
})

local Section = Tab:CreateSection("Sukuna No need item")

local Button = Tab:CreateButton({
 Name = "20x Big Malevolent Kitchen",
 Callback = function()
        for i = 1, 20 do
game:GetService("ReplicatedStorage").Events.shrine:FireServer()
end
   end    
})

local Button = Tab:CreateButton({
 Name = "20x Small Malevolent Kitchen",
 Callback = function()
        for i = 1, 20 do
game:GetService("ReplicatedStorage").RyomenSukuna.MalevolentShrine.Gaymalev:FireServer()
end
   end    
})

local Section = Tab:CreateSection("Gojo No need item - Destroy server for 5 - 10 min")

local Button = Tab:CreateButton({
 Name = "ATOMIC PURPLE",
 Callback = function()
        for i = 1, 10000 do
game:GetService("ReplicatedStorage").GojoV3.nuke:FireServer()
end
   end    
})

local Button = Tab:CreateButton({
 Name = "ATOMIC PURPLE - with chat",
 Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/IFeelPower/Battle/refs/heads/main/Purple"))()
   end    
})

local Section = Tab:CreateSection("Yamamoto")

local Button = Tab:CreateButton({
 Name = "FLAME Hell - need West Zanjitsu Gokui",
 Callback = function()
        for i = 1, 100 do
game:GetService("Players").LocalPlayer.Backpack["West Zanjitsu Gokui"].LocalScript.Event:FireServer()
end
   end    
})

local Tab = Window:CreateTab("Kill", 4483362458) -- Title, Image

local Section = Tab:CreateSection("Need Awaken Toji - you can awake toji in Main tab")

local Button = Tab:CreateButton({
 Name = "Kill Aura",
 Callback = function()
        while true do
  wait(1)
game:GetService("Players").LocalPlayer.Backpack["Wrong Way"].Main.Event:FireServer()
end
   end    
})

local Section = Tab:CreateSection("Kill Aura V2 - No need item")

local Button = Tab:CreateButton({
 Name = "Kill Aura V2 Slash",
 Callback = function()
        while true do
  wait(1)
game:GetService("ReplicatedStorage").Remotes.colovo:FireServer()
end
   end    
})

local Section = Tab:CreateSection("Need Gyutaro")

local Button = Tab:CreateButton({
 Name = "Kill Aura V3 - Hold Rampant Arc",
 Callback = function()
        while true do
  wait(0.1)
game:GetService("Players").LocalPlayer.Character:FindFirstChild("Rampant Arc").Keybind.Fire:FireServer()
end
   end    
})

local Tab = Window:CreateTab("Option", 4483362458) -- Title, Image

local Button = Tab:CreateButton({
 Name = "Back to Lazy script",
 Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/IFeelPower/Battle/refs/heads/main/Latest"))()
   end    
})

